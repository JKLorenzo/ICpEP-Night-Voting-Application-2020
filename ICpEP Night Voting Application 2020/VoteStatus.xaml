<Page x:Class="ICpEP_Night_Voting_Application_2020.VoteStatus"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ICpEP_Night_Voting_Application_2020"
      mc:Ignorable="d" 
      d:DesignHeight="588" d:DesignWidth="1360"
      Title="VoteStatus">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <Image Source="Resources/logo.png" Width="400" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Grid>

        <TextBlock Grid.Row="1" x:Name="textBlock" Text="VOTE SUBMITTED" FontSize="50" FontFamily="Courier New" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0 0 0 0"/>
    </Grid>
</Page>
